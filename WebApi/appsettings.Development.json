{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "EntraId": {
    "TenantId": "your-dev-tenant-id-here",
    "ClientId": "your-dev-client-id-here",
    "ClientSecret": "your-dev-client-secret-here",
    "RedirectUri": "https://localhost:7001/signin-oidc",
    "PostLogoutRedirectUri": "https://localhost:7001/signout-callback-oidc",
    "RequireHttps": false,
    "CookieSecure": false,
    "AdminUserPrincipalNames": [
      "admin@your-domain.onmicrosoft.com"
    ]
  }
}