# Example environment variables for Microsoft OAuth
# Copy this file to .env and replace with your actual values

# Microsoft Azure AD Application Configuration
VITE_AZURE_CLIENT_ID=12345678-1234-1234-1234-123456789012
VITE_AZURE_TENANT_ID=87654321-4321-4321-4321-210987654321
VITE_AZURE_REDIRECT_URI=http://localhost:5173/
VITE_AZURE_POST_LOGOUT_REDIRECT_URI=http://localhost:5173/login

# Microsoft Graph API Scopes
VITE_AZURE_GRAPH_SCOPES=User.Read

# Environment Configuration
VITE_NODE_ENV=development